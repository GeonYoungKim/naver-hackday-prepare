<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="paging">

	<select id="selectList" parameterType="com.hack.naver.model.Paging" resultType="hashmap">
		SELECT num,content
		FROM paging
		ORDER BY num DESC LIMIT #{no},#{unit};
	</select>
</mapper>